function showDescription(selObj){
	var desc = document.getElementById('theme_desc');
	var selected = selObj.options[selObj.selectedIndex];
	desc.innerHTML = selected.getAttribute('data-desc');
}

function validateEmail(elem){
		var retVal = true;
		var email = elem.value;
		if(email){
			// validate email format xxx@yy.dmdm
			var regex = /^(([^<>()\[\]\\.,;:\s@"]+(\.[^<>()\[\]\\.,;:\s@"]+)*)|(".+"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;

			if (regex.test(email)) {
				elem.style.borderColor = "";
				elem.style.backgroundColor = "";

			}
			else {
				// Invalid email
				elem.style.borderColor = "red";
				elem.style.backgroundColor = "#ff8080";

				//alert("Invalid email");
				retVal = false;
			}
		}
		else{

			elem.style.borderColor = "";
			elem.style.backgroundColor = "";

		}
		return retVal;
}

function validateForm() {
	var ret = true;
	var email_fields = document.getElementsByName("author_emails");
	if(email_fields!=null){
		for(var i=0; i<email_fields.length; i++){
			var email = email_fields[i].value;
			if (!validateEmail(email)) {
				email_fields[i].style.borderColor = "red";
				ret = false;
			}
			else{
				email_fields[i].style.borderColor = "";
			}
		}
	}
    if(ret === false){
    	alert("Please correct the errors on the form.");
    }
	return ret;
}

function addRow(tableID) {

	var table = document.getElementById(tableID);

	var rowCount = table.rows.length;
	var row = table.insertRow(rowCount);
	row.id = "D"+rowCount+"R";
	row.className="MoveableRow";
	var colCount = table.rows[0].cells.length;

	for(var i=0; i<colCount; i++) {

		var newcell	= row.insertCell(i);

		newcell.innerHTML = table.rows[1].cells[i].innerHTML;

		switch(newcell.childNodes[0].type) {
			case "text":
					newcell.childNodes[0].value = "";
					newcell.childNodes[0].readOnly = false;
					break;
			case "checkbox":
					newcell.childNodes[0].checked = false;
					break;
			case "number":
					newcell.childNodes[0].value = rowCount;
					break;
			case "email":
					newcell.childNodes[0].value = "";
					newcell.childNodes[0].readOnly = false;
					break;
			case "select-one":
					newcell.childNodes[0].selectedIndex = 0;
					break;
			case "button":
				var id = newcell.childNodes[0].getAttribute('id');
				var sub_id = id.substring(0, 1);
				var new_id = sub_id+rowCount;

				newcell.childNodes[0].setAttribute('id',new_id);
				if (sub_id == "U") {
					newcell.childNodes[0].className = "move_up";

				}
				else if (sub_id == "D") {
					newcell.childNodes[0].className = "move_down";

				}
				break;
		}
	}

}

function deleteRow(tableID) {
	try {
		var table = document.getElementById(tableID);
		var rowCount = table.rows.length;


		for(var i=1; i<rowCount; i++) {
			var row = table.rows[i];
			var chkbox = row.cells[0].childNodes[0];

			if(null != chkbox && true == chkbox.checked) {
				if (row.id == "D1R") {
					alert("Cannot delete current user author.");
					break;
				}
				if(rowCount <= 2) {
					alert("Cannot delete all the rows.");
					break;
				}

				table.deleteRow(i);
				rowCount--;
				i--;

				}

			}

		var colCount = table.rows[0].cells.length;

	}catch(e) {
		alert(e);
	}
}



jQuery(document).ready(function(){
    jQuery('#dataTable').on('click','input.move_up', function() {
       var currentRow = jQuery(this).closest('tr');
        if (jQuery(this).is(".move_up")) {
            currentRow.insertBefore(currentRow.prev());
        }
    });


    jQuery('#dataTable').on('click','input.move_down', function() {
        var currentRow = jQuery(this).closest('tr');
        if (jQuery(this).is(".move_down")) {
            currentRow.insertAfter(currentRow.next());
        }
    });

	       jQuery("#checkAll").change(function(){
	              jQuery(".checkbox1").prop('checked', jQuery(this).prop("checked"));
	       });

  jQuery("#editor1").on('keyup', function() {
    var words = this.value.match(/\S+/g).length;

    if (words > 250) {
      // Split the string on first 250 words and rejoin on spaces
      var trimmed = jQuery(this).val().split(/\s+/, 250).join(" ");
      // Add a space at the end to make sure more typing creates new words
      jQuery(this).val(trimmed + " ");
    }
    else {
      jQuery('#display_count').text(words);
      jQuery('#word_left').text(250-words);
    }
  });

  jQuery("#keywords").on('keyup', function() {
    if (this.value.match(/,+/g) != null)
    {
	var kwords = this.value.match(/,+/g).length;
	}
	else
	{
		var kwords = 0;
	}

    if (kwords > 9) {
      // Split the string on first 10 commas and rejoin on commas
      var trimmed = jQuery(this).val().split(/,+/, 10).join(",");
      // Add a comma at the end to make sure more typing creates new keywords
      jQuery(this).val(trimmed + ",");
      jQuery('#display_kcount').text(kwords);
      jQuery('#kword_left').text(10-kwords);
    }
    else {
      jQuery('#display_kcount').text(kwords);
      jQuery('#kword_left').text(10-kwords);
    }

  });
});


</script>


